<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bryan.hereheroemergency">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RequestActivity" />
        <activity android:name=".InformationActivity" />
        <activity android:name=".AccountActivity" />
        <activity android:name=".ReportActivity" />
        <activity android:name=".ListNumbersActivity" />
        <activity android:name=".EarthquakeActivity" />
        <activity android:name=".TsunamiActivity" />
        <activity android:name=".CarAccidentActivity" />
        <activity android:name=".CPRActivity" />
        <activity android:name=".TerroristActivity" />
        <activity android:name=".HouseFireActivity" />
        <activity android:name=".SirenActivity"></activity>
    </application>

</manifest>