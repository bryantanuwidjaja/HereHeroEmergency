            android:id="@+id/button_informationActivity_HouseFire"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginBottom="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:background="@color/numbersLight"
            android:elevation="6sp"
            android:padding="6sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.395" />

        <TextView
            android:id="@+id/textView20"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@color/numberDark"
            android:paddingTop="5dp"
            android:text="House Fire"
            android:textAlignment="center"
            android:textColor="@color/colorWhite"
            android:textSize="16dp"
            android:translationZ="11dp"
            app:layout_constraintBottom_toBottomOf="@+id/button_informationActivity_HouseFire"
            app:layout_constraintEnd_toEndOf="@+id/button_informationActivity_HouseFire"
            app:layout_constraintStart_toStartOf="@+id/button_informationActivity_HouseFire" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginBottom="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="16dp"
            android:translationZ="10dp"
            app:layout_constraintBottom_toTopOf="@+id/textView20"
