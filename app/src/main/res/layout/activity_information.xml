            android:layout_height="wrap_content"
            android:background="@color/numberDark"
            android:paddingTop="5dp"
            android:text="Earthquake"
            android:textAlignment="center"
            android:textColor="@color/colorWhite"
            android:textSize="16dp"
            android:translationZ="11dp"
            app:layout_constraintBottom_toBottomOf="@+id/button_InformationActivity_Earthquake"
            app:layout_constraintEnd_toEndOf="@+id/button_InformationActivity_Earthquake"
            app:layout_constraintStart_toStartOf="@+id/button_InformationActivity_Earthquake" />
    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
